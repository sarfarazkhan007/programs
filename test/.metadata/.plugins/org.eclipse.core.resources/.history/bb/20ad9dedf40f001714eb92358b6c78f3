import java.util.Scanner;

//import javax.swing.JFrame;
public class Hello 
{

	public static void main(String[] args) 
	{
		int a,n,c=0,p=0,csum=0,psum=0,ps=0,cs=0;
//		int ps[]=new int[n];
//		int cs[]=new int[n];
		Scanner sc=new Scanner(System.in);
		System.out.print("type the first smallest number = ");
		a=sc.nextInt();
		System.out.print("type the second larger number = ");
		n=sc.nextInt();
//		int ps[]=new int[n];
//		int cs[]=new int[n];
		for(int i=a;i<=n;i++)
		{
			for(int j=2;j<=i/2;j++)//(int)i/2;j=(j%2!=0)?(j+2):(j+1))
			{
				if(i%j==0)
				{
					cs+=a;
//					System.out.println(++temp +"helo");
//					cs[c++]=i;
//					System.out.print("not a prime number");
					break;
				}
				else if(j==(int)i/2)//(int)i/2||j==((int)i/2)-1)
				{
				ps+=a;
//					ps[p++]=i;
//				System.out.println(++temp +" a prime number");
				}
			}
     	}
//			for(int i=0;i<cs.length;i++){
//				System.out.println(cs[i]);
//				csum=csum+cs[i];
//			}
//			for(int i=0;i<ps.length;i++){
//				System.out.println(ps[i]);
//				psum=psum+ps[i];
//			}
//		System.out.print(csum +" "+psum);
			System.out.print(c + "     	"+ps);
	}
}

